# Ignore Python virtual environments
venv/
.env/

# Ignore Python cache files
*.pyc
__pycache__/

# Ignore environment variable files
.env

# Ignore log files
*.log

# Ignore compiled binaries
*.o
*.out
*.exe

# Ignore OS-specific files
.DS_Store
Thumbs.db

