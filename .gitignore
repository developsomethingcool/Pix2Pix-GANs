edges/*
real_images/*
visualization_results/*
generated_images/*
evaluation_results/*

# Keep the folder structure (but not the files)
!edges/.gitkeep
!real_images/.gitkeep
!visualization_results/.gitkeep
!generated_images/.gitkeep
!evaluation_results/.gitkeep
